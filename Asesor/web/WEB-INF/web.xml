<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>CrearPersona</servlet-name>
        <servlet-class>CONEXION.CrearPersona</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearJuego</servlet-name>
        <servlet-class>CONEXION.CrearJuego</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearAlquiler</servlet-name>
        <servlet-class>CONEXION.CrearAlquiler</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarPrecio</servlet-name>
        <servlet-class>CONEXION.ModificarPrecio</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InformeVenta</servlet-name>
        <servlet-class>CONEXION.InformeVenta</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CrearPersona</servlet-name>
        <url-pattern>/CrearPersona</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearJuego</servlet-name>
        <url-pattern>/CrearJuego</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearAlquiler</servlet-name>
        <url-pattern>/CrearAlquiler</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarPrecio</servlet-name>
        <url-pattern>/ModificarPrecio</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InformeVenta</servlet-name>
        <url-pattern>/InformeVenta</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Principal.jsp</welcome-file>
    </welcome-file-list>
</web-app>
